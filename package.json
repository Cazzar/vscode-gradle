{
	"name": "Gradle",
	"description": "Run gradle tasks from within Visual Studio Code",
	"version": "1.0.0",
	"publisher": "cazzar09",
	"icon": "icon.png",
	"readme": "README.md",
	"author": {"name": "Cayde Dixon", "url": "http://cazzar.net", "email": "me@cazzar.net"},
	"maintainers": [{"name": "Cayde Dixon", "url": "http://cazzar.net", "email": "me@cazzar.net"}],
	"bugs": {
		"email": "me@cazzar.net",
		"url": "https://github.com/Cazzar/vscode-gradle/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Cazzar/vscode-gradle/"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:gradle"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [{
			"command": "gradle",
			"title": "Gradle"
		}],
		"configuration": {
			"type": "object",
			"title": "Gradle configuration",
			"properties": {
				"gradle.useCommand": {
					"type": "string",
					"default": "gradlew",
					"description": "use this command to run gradle."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}